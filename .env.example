# Oracle 9.1 Protocol Development Kit Environment Configuration

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_BASE=https://api.openai.com/v1

# Mem0 Configuration
MEM0_API_URL=http://localhost:8888
MEM0_API_KEY=your-mem0-api-key-here

# Database Configuration
POSTGRES_PASSWORD=oracle_secure_password_2024
POSTGRES_DB=oracle_nexus
POSTGRES_USER=postgres

# Neo4j Configuration
NEO4J_PASSWORD=oracle_neo4j_password_2024
NEO4J_USERNAME=neo4j

# Vector Database Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-api-key-here

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=oracle_redis_password_2024

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=oracle_elastic_password_2024

# MinIO Configuration
MINIO_ACCESS_KEY=oracle_minio_access_key
MINIO_SECRET_KEY=oracle_minio_secret_key_2024

# Monitoring Configuration
GRAFANA_PASSWORD=oracle_grafana_password_2024
PROMETHEUS_RETENTION=200h

# Oracle 9.1 Protocol Configuration
ORACLE_MEMORY_RETENTION_DAYS=365
ORACLE_MEMORY_MAX_TOKENS=10000
ORACLE_ANALYSIS_DEPTH=comprehensive
ORACLE_PROTOCOL_VERSION=9.1

# Security Configuration
JWT_SECRET_KEY=oracle_jwt_secret_key_2024_very_secure
ENCRYPTION_KEY=oracle_encryption_key_2024_32_chars
SESSION_SECRET=oracle_session_secret_2024

# Application Configuration
FLASK_ENV=production
FLASK_DEBUG=false
FLASK_SECRET_KEY=oracle_flask_secret_2024

# Frontend Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_WS_URL=ws://localhost:5000
REACT_APP_ENVIRONMENT=production

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=/app/logs/oracle.log

# Performance Configuration
WORKER_PROCESSES=4
WORKER_CONNECTIONS=1000
MAX_REQUESTS_PER_WORKER=1000
TIMEOUT=30

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
CACHE_BACKEND=redis

# File Upload Configuration
MAX_UPLOAD_SIZE=100MB
ALLOWED_EXTENSIONS=pdf,doc,docx,txt,mp3,wav,mp4

# Email Configuration (Optional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-email-password
SMTP_USE_TLS=true

# Webhook Configuration (Optional)
WEBHOOK_SECRET=oracle_webhook_secret_2024
WEBHOOK_URL=https://your-domain.com/webhooks

# Third-party Integrations (Optional)
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
TEAMS_WEBHOOK_URL=https://your-teams-webhook-url
ZOOM_API_KEY=your-zoom-api-key
ZOOM_API_SECRET=your-zoom-api-secret

# Development Configuration
DEV_MODE=false
DEBUG_TOOLBAR=false
PROFILER_ENABLED=false

# Testing Configuration
TEST_DATABASE_URL=postgresql://postgres:test@localhost:5432/oracle_test
TEST_REDIS_URL=redis://localhost:6379/1

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=oracle-nexus-backups
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# SSL Configuration
SSL_ENABLED=true
SSL_CERT_PATH=/etc/ssl/certs/oracle.crt
SSL_KEY_PATH=/etc/ssl/private/oracle.key

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,https://your-domain.com
CORS_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_HEADERS=Content-Type,Authorization

# Health Check Configuration
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10

# Metrics Configuration
METRICS_ENABLED=true
METRICS_PORT=8080
METRICS_PATH=/metrics

