# Intelligence OS Platform - GitHub Codespace Environment

# Environment
NODE_ENV=development
ENVIRONMENT=codespace
DEBUG=true

# Application URLs (will be updated by Codespace)
FRONTEND_URL=https://localhost-3000.app.github.dev
API_URL=https://localhost-8000.app.github.dev
VOICE_PROCESSOR_URL=https://localhost-5000.app.github.dev

# Database Configuration
DATABASE_URL=postgresql://intelligence_user:intelligence_pass@postgres:5432/intelligence_os
REDIS_URL=redis://redis:6379

# Authentication
JWT_SECRET=codespace-development-jwt-secret-key
JWT_EXPIRES_IN=24h

# AI Services (Add your keys here)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# External Integrations (Optional for development)
ZAPIER_WEBHOOK_SECRET=codespace-zapier-webhook-secret
NOTION_API_KEY=your-notion-api-key-here
DART_API_KEY=your-dart-api-key-here

# Monitoring
LOG_LEVEL=debug
ENABLE_METRICS=true
PROMETHEUS_PORT=9090

# Voice Processing
VOICE_MODEL_PATH=./models/voice
ENABLE_SPEAKER_ID=true
AUDIO_SAMPLE_RATE=16000

# Security
RATE_LIMIT_REQUESTS=1000
RATE_LIMIT_WINDOW_MS=60000
CORS_ORIGIN=*

# Features
ENABLE_CACHING=true
ENABLE_REAL_TIME_PROCESSING=true
ENABLE_AI_ORCHESTRATION=true

# Codespace specific
CODESPACE_NAME=${CODESPACE_NAME:-localhost}
GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN=${GITHUB_CODESPACES_PORT_FORWARDING_DOMAIN:-app.github.dev}