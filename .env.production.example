# Oracle Intelligence System Environment Configuration
# Copy this to .env and add your actual API keys

# OpenAI API Key (required for Mem0 and Oracle intelligence)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Mem0 specific configuration
MEM0_API_KEY=your_mem0_api_key_here

# Oracle System Configuration
ORACLE_ENVIRONMENT=development
ORACLE_LOG_LEVEL=INFO

# Impact Launchpad Configuration
ORGANIZATION_NAME=Impact Launchpad
TEAM_SIZE=7

